			width: 10%;
			padding: 20px;
			text-align: center;
		}
		
		.col-service {
			width: 50%;
			padding: 20px 0;
			
			strong {
				color: $dark-blue;
			}
			
			p {
				margin: 0 0 10px 0;
			
				&:last-child {
				margin: 0;
				}
			}
		}
		
		.col-price {
			width: 20%;
			padding: 20px 10px 20px 20px;
			text-align: right;
			font-size: 22px;
			font-weight: 600;
			color: $red;
		}
		
		.col-points {
			width: 20%;
			padding: 20px 20px 20px 10px;
			text-align: right;
			font-size: 22px;
			font-weight: 600;
			color: $red;
		}
	}
}


/* MAKE A RESERVATION DETAILS */
/* ------------------------------------------------------- */
#make-a-reservation-details {

	@include overflowfix;
	
	.full-width input.datepicker {
		width: 48%;
		margin-right: 4%;
		float: left;
		background: #eaeaea url("../images/icon-calendar.png") center right 6px no-repeat;
		background-size: 13px 13px;
	}
	
	.full-width select {
		width: 48%;
		float: left;
	}
}


/* EDIT A RESERVATION DETAILS */
/* ------------------------------------------------------- */
#edit-reservation-search-results span.search-criteria {
	color: $grey;
	display: inline-block;
	margin: 0 5px;
}
.delete-cart-item {
	cursor: pointer;
}


/* APPLICATION FORM */
/* ------------------------------------------------------- */
.standard-form#application-form {

	input[type="text"], input[type="password"], select, textarea {
		background: #fff;
		resize: none;
	}
	
	input[type="button"] {
		float: left;
		clear: left;
	}
}


/* POST TRIP SURVEY */
/* ------------------------------------------------------- */
#post-trip-survey {

	label.half-width {
		float: left; 
		clear: left;
		padding: 0 20px 0 0;
	}
	
	.contact-checkbox {
		
		input {
			display: inline-block;
		
			&:last-child {
				margin: 0 0 0 15px;
			}
		}
	}
}

#star-rating {
	width: 50%;
	float: right;
	padding: 0 0 0 20px;
	@include box-sizing;

	table {
		width: 100%;
		border-collapse: collapse;
		table-layout: fixed;
		margin: 19px 0 0 0;
		
		tr {
			border-bottom: 1px solid $light-grey-darker;
			
			&:first-child {
				border-top: 1px solid $light-grey-darker;
			}
		}
		
		td {
			width: 46%;
			padding: 8px 2%;
			line-height: 1;
			
			&.label-cell {
				text-align: right;
				font-weight: 700;
				color: $red;
				text-transform: uppercase;
				font-size: 12px;
			}
			
			&.stars-cell {
				
				a {
					display: inline-block;
					width: 16px;
					height: 16px;
					background: url("../images/star-blank.png") center no-repeat;
					background-size: 16px 16px;
					
					&.active, &:hover {
						background: url("../images/star-selected.png") center no-repeat;
						background-size: 16px 16px;
					}
				}
			}
		}
	}
}


/* EXPRESS CLUB ACCOUNT PAGES */
/* ------------------------------------------------------- */

.total-points {

	span {
		display: inline-block;
		font-size: 20px;
		font-weight: 700;
		color: $red;
		margin: 0 2px;
		position: relative;
		top: 2px;
	}
}

.express-club-member-page {
	.bar-content p:last-child {
		margin-bottom: 0;
	}
}

.edit-account-info-links {
	color: #cccccc;
	text-transform: uppercase;
	font-size: 12px;
}


/* HOME PAGE */
/* ------------------------------------------------------- */

section.homepage {
	@include box-sizing;
	@include overflowfix;
}

.homepage-top-box {
	width: 100%;
	height: 450px;
	padding: 30px 0;
	background: url("../images/home-bg.jpg") center no-repeat;
	
	.wrapper {
		height: 100%;
		@include overflowfix;
	}
	
	#search-box {
		@include box-sizing;
		height: 100%;
		background: #fff;
		width: 35%;
		float: left;
		padding: 20px;
		border: 8px solid rgba(255,255,255,0.4);
		-webkit-background-clip: padding-box;
		-moz-background-clip: padding-box;
		background-clip: padding-box;
		@include box-shadow(1px, 1px, 3px, rgba(0,0,0,0.1));
		
		h2 {
			font-size: 28px;
			font-weight: 700;
			line-height: 1;
			margin: 0 0 15px 0;
		}
		
		p {
			font-size: 18px;
			margin: 0 0 15px 0;
		}
		
		hr {
			height: 2px;
			background: $light-grey;
			margin: 15px 0 20px;
		}
		
		.bottom-button-group {
			margin-top: 11px;
		}
	}
	
	#homepage-slideshow {
		@include box-sizing;
		width: 62%;
		height: 100%;
		float: right;
		background: #fff;
		border: 8px solid rgba(255,255,255,0.4);
		-webkit-background-clip: padding-box;
		-moz-background-clip: padding-box;
		background-clip: padding-box;
		@include box-shadow(1px, 1px, 3px, rgba(0,0,0,0.1));
		position: relative;
		
		.slides > li {
			position: relative;
		}
		
		.slide-overlay {
			@include box-sizing;
			width: 455px;
			height: 100%;
			padding: 60px 20px 40px 140px;
			position: absolute;
			right: 0;
			background: url("../images/home-slider-gradient.png") right repeat-y;
			z-index: 2;
			text-align: center;
			
			h3 {
				color: #fff;
				font-size: 24px;
			}
			
			p {
				color: #fff;
				font-size: 16px
			}
			
			.mid-button {
				border: 4px solid rgba(0,0,0,0.2);
				-webkit-background-clip: padding-box;
				-moz-background-clip: padding-box;
				background-clip: padding-box;
			}
		}
	}
}

.homepage-search-form {

	label.half-width {
		width: 48%;
		float: left;
		@include box-sizing;
		padding: 0;
		margin: 0 0 10px 0;
		
		&:nth-child(2n) {
			margin: 0 0 0 4%;
		}
	}
	
	label.full-width, span.label.full-width {
		width: 100%;
		float: left;
		@include box-sizing;
		padding: 0 0 0 0;
		margin: 0 0 10px 0;
		@include overflowfix;
		
		input {
			width: 48%;
			float: left;
			margin: 0 4% 0 0;
		}
		
		select {
			width: 48%;
			float: left;
		}
	}
	
	label input.datepicker, span.label input.date-picker {
		background: #eaeaea url("../images/icon-calendar.png") center right 6px no-repeat;
		background-size: 13px 13px;
	}
}

.homepage-callouts {
	@include wrapper;
	padding: 60px 0;
	border-bottom: 1px solid $light-grey;
	
	.callout {
		float: left;
		@include box-sizing;
		width: 22.75%;
		margin: 0 3% 0 0;
		text-align: center;

		&#callout-four {
			margin: 0;
		}
		
		h3 {
			margin: 15px 0;
		}
	}
}

.homepage-testimonials-faqs {
	@include wrapper;
	padding: 60px 0;
	@include overflowfix;
	
	#testimonials {
		width: 35%;
		@include box-sizing;
		padding: 0 30px 0 0;
		border-right: 1px solid $light-grey;
		float: left;
		
		> h3 {
			color: $dark-blue;
			text-align: center;
		}
		
		.testimonial-item {
			margin: 0 0 20px 30px;
			padding: 0 0 20px 0;
			border-bottom: 1px solid $light-grey;
			
			&:before {
				content: " ";
				display: block;
				width: 22px;
				height: 20px;
				background: url("../images/quote.png") center no-repeat;
				background-size: 22px 20px;
				position: absolute;
				left: -2px;
			}
			
			&:last-child {
				border-bottom: 0;
				margin-bottom: 0;
				padding-bottom: 0;
			}
		}
		
		p {
			margin: 0 0 15px 0;
		}
		
		span.name {
			display: block;
			font-size: 12px;
			color: $red;
			font-weight: 600;
			text-align: right;
		}
	}
	
	#accordion-wrapper {
		width: 65%;
		@include box-sizing;
		padding: 0 0 0 30px;
		float: right;
	
		> h3 {
			color: $dark-blue;
			text-align: center;
		}
	}
}

.homepage-partners {
	padding: 40px 0 20px;
	background: #fff;
	border-top: 1px solid $light-grey;
	text-align: center;
	
	h3 {
		color: $dark-blue;
	}
	
}







