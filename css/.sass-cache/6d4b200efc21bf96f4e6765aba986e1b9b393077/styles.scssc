}

.edit-links {
	float: right;
	font-size: 12px;
	position: relative;
	top: 1px;	
	opacity: 0.8;

	a {
		color: #fff;
		
		&:hover {
			color: $dark-blue;
		}
	}
}

#grand-total {
	width: 26%;
	position:relative;
	right: 0;
	left: 74%;
	@include box-sizing;
	padding: 0 20px 0 0;
	
	.label {
		display: block;
		float: left;
		width: 70%;
		padding: 0 20px 0 0;
		margin: 0;
		@include box-sizing;
		font-size: 21px;
		color: $dark-blue;
		font-weight: 700;
	}
		
	.price {
		display: block;
		float: right;
		width: 30%;
		padding: 0;
		margin: 0;
		@include box-sizing;
		font-size: 21px;
		color: $red;
		font-weight: 700;
		text-align: right;
	}
}

.cart-page.bottom-button-group {
	@include overflowfix;
	margin: 30px auto 10px;
	float: left;
}

#card-expiration-month {
	float: left;
	width: 70%;
	margin: 0 5% 0 0;
}

#card-expiration-year {
	float: left;
	width: 25%;
}
	

/* CAR CARE SERVICES */
/* ------------------------------------------------------- */
#car-care-services-table {

	margin: 0 0 30px 0;
	@inlcude overflowfix;

	.car-care-row {
		background: $light-grey;
		@include overflowfix;			
		
		&:nth-child(2n) {
			background: $light-grey-darker;
		}
		
		> div {
