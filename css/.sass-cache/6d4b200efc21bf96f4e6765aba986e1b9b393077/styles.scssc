@import "variables-mixins";


/* TYPOGRAPHY */
/* ------------------------------------------------------- */
body {
	font: 400 14px/1.4 "open sans", arial, helvetica, sans-serif;
	color: #000;
}

p {
	margin: 0 0 20px 0;
}

ul {
	list-style-type: disc;
	margin: 0 0 15px 27px;
}

li {
	margin: 0 0 5px 0;
}

strong {
	font-weight: 700;
}

em {
	font-style: italic;
}

small {
	font-size: 11px;
	line-height: 1.35;
	display: block;
}

a {
	color: $light-blue;
	text-decoration: none;
	
	&:hover {
		text-decoration: none;
		color: $dark-blue;
	}
}

h1 {
	display: block;
	width: 100%;
	position: absolute;
	margin: 45px 0 0 0;
	@include box-sizing;
	background: rgba(255, 255, 255, 0.65);
	padding: 20px 30px;
	font-size: 28px;
	font-weight: 700;
	line-height: 1;
	text-align: left;
	text-transform: uppercase;
	color: $dark-blue;
	@include box-shadow(1px, 1px, 2px, rgba(0,0,0,0.25));
}

h2 {
	font-weight: 600;
	font-size: 26px;
	color: $dark-blue;
	margin: 0 0 20px 0;
}

h3 {
	font-weight: 600;
	font-size: 20px;
	color: $red;
	margin: 0 0 20px 0;
}

h4 {
	@include light-blue-gradient;
	@include box-sizing;
	-webkit-border-top-left-radius: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-topleft: 4px;
	-moz-border-radius-topright: 4px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	line-height: 1;
	padding: 8px 20px;
	font-size: 14px;
	font-weight: 600;
	text-transform: uppercase;
	color: #fff; 
}

h5 {
	font-size: 16px;
	font-weight: 600;
	text-transform: uppercase;
	color: $dark-blue;
	margin: 0 0 12px 0;
}

h6 {
	text-transform: uppercase;
	color: $red;
	font-weight: 700;
	font-size: 14px;
	margin: 0 0 15px 0;
}

/* IMAGES */
/* ------------------------------------------------------- */

img {

	&.float-right {
		float: right;
		margin: 0 0 20px 20px;
	}
	
	&.float-left {
		float: left;
		margin: 0 20px 20px 0;
	}
}


/* REUSABLE CLASSES */
/* ------------------------------------------------------- */

.wrapper {
	@include wrapper;
}

hr {
	border: none;
	height: 1px;
	background: $grey;
	padding: 0;
	margin: 20px 0;
	clear: both;
}

.bottom-button-group {
	@include overflowfix;
	margin: 10px auto;
	width: 100%;
	text-align: center;
	float: left;
}

.small-button {
	@include small-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}

.mid-button {
	@include mid-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}


.jumbo-button {
	@include jumbo-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}



/* PAGE LAYOUTS */
/* ------------------------------------------------------- */
#page-content {
	@include overflowfix;
	@include wrapper;
	padding: 60px 0;
	
	#left {
		float: left;
		@include box-sizing;
	}
		
	#right {
		float: right;
		@include box-sizing;
	}
}

#page-content.fifty-fifty-layout {
	
	#left {
		width: 50%;
		padding: 0 40px 0 0;
		border-right: 1px solid $light-grey;
	}
	
	#right {
		width: 50%;
		padding: 0 0 0 40px;
	}
}

#page-content.left-sidebar-layout {
	#left {
		width: 25%;
		
		aside.summary {
			margin: 0 0 40px 0;
		}
	}
	
	#right {
		width: 75%;
		padding: 0 0 0 40px;
	}
}

#page-content.right-sidebar-layout {
	#left {
		width: 75%;
		padding: 0 40px 0 0;
	}
	
	#right {
		width: 25%; 
	}
}

#page-content.full-width-layout {
	#center {
		width: 100%;
		padding: 0;
	}
}




/* HEADER */
/* ------------------------------------------------------- */
header {
	position: relative;
	height: 90px;
	
	/* start secondary nav */
	#secondary-nav {
		@include wrapper;
		
		#logo {
			position: relative;
			z-index:2;
			float: left;
		}
		
		span.reserve-by-phone {
			float: left;
			margin: 13px 0 0 0;
			font-size: 15px;
		}
		
		#register {
			float: right;
			font-weight: 600;
			margin: 15px 0 0 10px;
		}
		
		#customer-login {
			float: right;
			margin: 11px 0 0 0;
			@include small-button;
			@include light-blue-gradient;
			
			&:hover {
				@include light-blue-gradient-hover;
			}
		}
	} /* end secondary nav */
	
	/* start main nav */
	.nav {
		position:absolute;
		bottom: 0;
		left: 0;
		right: 0;
		display:inline-block;
		width: 100%;
		@include red-gradient;
		
		ul {
			margin: 0 auto;
			list-style-type: none;
			
			li {
				margin: 0;
			}
		}
	}
		
	.nav-list {
		@include wrapper;
		text-align: right;
	}
	
	.nav-item {
		display: inline-block;
		zoom:1;
		position:relative;
		font-size: 15px;
		line-height: 1;

		a {
			display: inline-block;
			padding: 11px 23px 13px;
			color: #fff;
			@include text-shadow(1px, 1px, rgba(0,0,0,0.15));

			&:hover {
				@include red-gradient-hover;
			}
			
			&#cart {
				background: #850005;
				
				&:hover {
					@include red-gradient-hover;
				}
			}
		}
		
		&.active > a, &.active > a#cart {
			@include red-gradient-hover;
		}
	}
	
	.nav-item:hover .nav-submenu {
		display: block;
	}
	
	.nav-submenu {
		display: none;
		position: absolute;
		left: 0;
		width: 225px;
		text-align: left;
		background: #b62126;
		z-index: 5;
		line-height: 1.25;
		
		a {
			display: block;
			font-size: 14px;
			padding: 8px 23px 8px;
		
			&:hover {
				background: #d0262b;
			}
		}
	}
	
	/* Mobile navigation */
	.nav-mobile {
		display:none; /* Hide from browsers that don't support media queries */
	}
	/* Mobile navigation, clickable area for revealing <ul> */
	.nav-click {
		position:absolute;
		top:0;
		right:0;
		display:none;
		height:34px;
		width:34px;
		cursor:pointer;
	}
	.nav-click i {
		display:block;
		height:34px;
		width:34px;
		background:#850005 url("../images/nav-arrow.png") no-repeat center center;
		background-size:16px;
	}
	.nav-click:hover {
		background-color:#6E1656;
	}
	.nav-rotate {
		-webkit-transform:rotate(180deg);
		-moz-transform:rotate(180deg);
		-ms-transform:rotate(180deg);
		-o-transform:rotate(180deg);
		transform:rotate(180deg);
	}
}

/* customer login dialog */
#customer-login-form {

	@include overflowfix;
	margin: 10px 0;

	input[type="text"], input[type="password"] {
		width: 48%;
		float: left;
		@include box-sizing;
		margin: 0;
		padding: 5px;
		font-size: 11px;
		-webkit-appearance: none;
		border: none;
		background: $light-grey;
		@include box-shadow-inset(0px, 1px, 3px, rgba(0, 0, 0, 0.15));
		
		&:focus {
			outline: 2px solid $light-blue;
			border: 0;
		}
	}
	
	input[type="text"] {
		margin: 0 4% 0 0;
	}
	
	#login-links {
		margin: 15px 0 0 0;
		float: left;
		width: 100%;
		text-align: center;
		
		a {
			font-size: 12px;
			text-transform: uppercase;
			color: $light-blue;
		}
	}
}



/* PAGE TITLE */
/* ------------------------------------------------------- */

#page-title {
	width: 100%;
	height: 150px;
	background: url("../images/title-bg.jpg");
	
}


/* FOOTER */
/* ------------------------------------------------------- */
footer {
	@include overflowfix;
	background: $red;
	
	#foot-one, #foot-two, #foot-three {
		float: left;
		color: #fff;
		
		a {
			display: block;
			margin: 0 0 10px 0;
			color: #fff;
			
			&:hover {
				text-decoration: underline;
			}
		}
	}
	
	#foot-one {
		font-size: 13px;
		line-height: 1.5;
		margin: 30px 0;
	}
	
	#foot-two {
		padding: 0 0 0 20px;
		margin: 30px 0 30px 20px;
		border-left: 1px solid #991b1f;
	}
	
	#foot-three {
		padding: 0 0 0 20px;
		margin: 30px 20px;
		border-left: 1px solid #991b1f;
	}
	
	#travel-agents {
		@include box-sizing;
		background: #991b1f;
		padding: 30px 30px 0;
		float: right;
		width: auto;
		height: 167px;
		text-align: left;
		
		h3 {
			text-transform: uppercase;
			font-weight: 700;
			color: #fff;
		}
		
		#travel-agents-login {
			
			input[type="text"], input[type="password"] {
				width: 110px;
				margin: 0 5px 0 0;
				padding: 5px;
				-webkit-appearance: none;
				border: 1px solid #8d0005;
				@include box-shadow-inset(1px, 1px, 3px, rgba(0,0,0,0.2));
			}
			
			input.small-button.red-button {
				border-color: #8d0005;
			}
		}
		
		#travel-agents-links {
			margin: 5px 0 0 0;
		}
		
		#travel-agents-links a {
			display: inline-block;
			color: #c7a3a5;
			font-size: 12px;
			text-transform: uppercase;
			
			&:first-child {
			margin: 0 15px 0 0;
			padding: 0 15px 0 0;
			border-right: 1px solid #8d0005;
			}
		}
	}
}


/* SIDE BAR */
/* ------------------------------------------------------- */

.bar-content {
	@include overflowfix;
	@include box-sizing;
	padding: 20px;
	background: $light-grey;
	border-top: 0;
	-webkit-border-bottom-left-radius: 4px;
	-webkit-border-bottom-right-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	-moz-border-radius-bottomright: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	
	p.pricing-details {
		@include overflowfix;
		font-weight: 700;
	
		span.left {
			float: left;
		}
		
		span.right {
			float: right;
			color: $red;
		}
		
		span.coupon-code {
			display: block;
			float: left;
			font-weight: 400;
		}
		
		&.grand-total {
	
			span.left {
				text-transform: uppercase;
				font-weight: 700;
				line-height: 1.6;
			}
			
			span.right {
				font-size: 20px;
				font-weight: 700;
				line-height: 1;
			}
		}
	}
	
	img.barcode {
		display: block;
		margin: 0 auto 20px;
	}
}


/* STANDARD FORM STYLES */
/* ------------------------------------------------------- */
.standard-form {
	width: 100%;
	@include overflowfix;
	
	fieldset {
		margin: 0 0 20px 0;
		width: 100%;
		float: left;
	}
	
	label, span.label {
		width: 50%;
		float: left;
		@include box-sizing;
		padding: 0 20px 0 0;
		margin: 0 0 20px 0;
		
		&:nth-child(2n) {
			padding: 0 0 0 20px;
		}
		
		span {
			display: block;
			text-transform: uppercase;
			color: $red;
			font-weight: 700;
			font-size: 12px;
			margin: 0 0 3px -1px;
		}
		
		input[type="text"], input[type="password"], select, textarea {
			-webkit-appearance: none;
			width: 100%;
			@include box-sizing;
			margin: 0;
			padding: 5px;
			border: none;
			background: $light-grey;
			@include box-shadow-inset(0px, 1px, 3px, rgba(0,0,0,0.15));
			@include border-radius(0);
			
			&:focus {
				outline: 2px solid $light-blue;
				border: 0;
			}
			
		}
		
		select {
			background: url("../images/tiny-blue-arrow.png") center right 8px no-repeat $light-grey;
			background-size: 8px 7px;
			cursor: pointer;
			-moz-appearance: none;
		    text-indent: 0.01px;
		    text-overflow: '';
		    
		    &.ff-height-fix {
			    height: 23px;
			    padding: 3px 5px;
		    }
		}
		
		&.selection-type {
			width: 45%;
			padding: 0 0 0 20px;
		}
		
		&.qty {
			width: 15%;
			padding: 0 0 0 20px;
		}
	}
	
	p {
		float: left;
	}
}

.express-club-quick-login {

	label {
		width: 40%;
	}
	
	input[type="button"] {
		margin: 16px 0 0 5%;
		width: 15%;
	}
	
	p {
		float: none;
	}
}



/* BREADCRUMBS */
/* ------------------------------------------------------- */
#breadcrumbs {

	@include overflowfix;
	margin: 0 0 55px 0;
	
	a {
		float: left;
		text-transform: uppercase;
		font-weight: 600;
		
		&#current-breadcrumb {
			color: $dark-blue;
			cursor: default;
		}
	}
	
	img {
		float: left;
		margin: 0 15px;
	}
}



/* SEARCH RESULTS */
/* ------------------------------------------------------- */
.search-results {
	.result {
		background: $light-grey;
		@include box-sizing;
		padding: 20px;
		float: left;
		text-align: center;
		border: 4px solid $light-grey-darker;
		@include border-radius(4px);
		position: relative;
		
		&:last-child {
			border-color: $light-blue;
			
			&:after {
				content:"";
				display: block;
				position: absolute;
				width: 60px;
				height: 60px;
				background: url("../images/best-rate.png") center no-repeat;
				background-size: 100%;
				top: -22px;
				left: -18px;
			}
		}
		
		h2 {
			font-weight: 700;
		}
		
		h3 {
			font-size: 18px;
		}
	}
}

.search-results.two-rates {
	.result {
		width: 48%;
		
		&:first-child {
			margin: 0 4% 0 0;
		}
	}
}

.search-results.three-rates {
	.result {
		width: 31%;
		margin: 0 3.5% 0 0;
		
		&:last-child {
			margin: 0;
		}
	}
}

/* CONFIRM DETAILS/PAYMENT */
/* ------------------------------------------------------- */
.confirm-details {

	@include overflowfix;
	width: 50%;
	@include box-sizing;
	
	&.left-col {
		float: left;
		padding: 0 20px 0 0;
	}
	
	&.right-col {
		float: right;
		padding: 0 0 0 20px;
	}
	
	h6 {
		font-size: 12px;
		margin: 0 0 3px 0;
	}
}

	

/* SHOPPING CART */
/* ------------------------------------------------------- */

.cart-item {
	margin: 0 0 30px 0;
}

.bar-content {
	@include overflowfix;
}

.cart-details {
	width: 75%;
	@include box-sizing;
	padding: 0 40px 0 0;
	float: left;
	
	p:last-child {
		margin: 0;
	}
}

.cart-prices {
	width: 25%;
	@include box-sizing;
	padding: 0;
	float: right;
	@include overflowfix;
	
	ul {
		display: block;
		width: 100%;
		margin: 0 0 5px 0;
		padding: 0;
		@include overflowfix;
		
		li.label {
			display: block;
			float: left;
			width: 70%;
			padding: 0 20px 0 0;
			margin: 0;
			@include box-sizing;
			font-size: 15px;
			color: $dark-blue;
			font-weight: 600;
		}
		
		li.price {
			display: block;
			float: right;
			width: 30%;
			padding: 0;
