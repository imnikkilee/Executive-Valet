@import "variables-mixins";


/* TYPOGRAPHY */
/* ------------------------------------------------------- */
body {
	font: 400 14px/1.4 "open sans", arial, helvetica, sans-serif;
	color: #000;
}

p {
	margin: 0 0 20px 0;
}

ul {
	list-style-type: disc;
	margin: 0 0 15px 27px;
}

li {
	margin: 0 0 5px 0;
}

strong {
	font-weight: 700;
}

em {
	font-style: italic;
}

small {
	font-size: 11px;
	line-height: 1.35;
	display: block;
}

a {
	color: $light-blue;
	text-decoration: none;
	
	&:hover {
		text-decoration: none;
		color: $dark-blue;
	}
}

h1 {
	display: block;
	width: 100%;
	position: absolute;
	margin: 45px 0 0 0;
	@include box-sizing;
	background: rgba(255, 255, 255, 0.65);
	padding: 20px 30px;
	font-size: 28px;
	font-weight: 700;
	line-height: 1;
	text-align: left;
	text-transform: uppercase;
	color: $dark-blue;
	@include box-shadow(1px, 1px, 2px, rgba(0,0,0,0.25));
}

h2 {
	font-weight: 600;
	font-size: 26px;
	color: $dark-blue;
	margin: 0 0 20px 0;
}

h3 {
	font-weight: 600;
	font-size: 20px;
	color: $red;
	margin: 0 0 20px 0;
}

h4 {
	@include light-blue-gradient;
	@include box-sizing;
	-webkit-border-top-left-radius: 4px;
	-webkit-border-top-right-radius: 4px;
	-moz-border-radius-topleft: 4px;
	-moz-border-radius-topright: 4px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	line-height: 1;
	padding: 8px 20px;
	font-size: 14px;
	font-weight: 600;
	text-transform: uppercase;
	color: #fff; 
}

h5 {
	font-size: 16px;
	font-weight: 600;
	text-transform: uppercase;
	color: $dark-blue;
	margin: 0 0 12px 0;
}

h6 {
	text-transform: uppercase;
	color: $red;
	font-weight: 700;
	font-size: 14px;
	margin: 0 0 15px 0;
}

/* IMAGES */
/* ------------------------------------------------------- */

img {

	&.float-right {
		float: right;
		margin: 0 0 20px 20px;
	}
	
	&.float-left {
		float: left;
		margin: 0 20px 20px 0;
	}
}


/* REUSABLE CLASSES */
/* ------------------------------------------------------- */

.wrapper {
	@include wrapper;
}

hr {
	border: none;
	height: 1px;
	background: $grey;
	padding: 0;
	margin: 20px 0;
	clear: both;
}

.bottom-button-group {
	@include overflowfix;
	margin: 10px auto;
	width: 100%;
	text-align: center;
	float: left;
}

.small-button {
	@include small-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}

.mid-button {
	@include mid-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}


.jumbo-button {
	@include jumbo-button; 
	
	&:hover {
		color: #fff;
	}
	
	&.red-button {
		@include red-gradient;
		
		&:hover {
			@include red-gradient-hover;
		}
	}
		
	&.light-blue-button {
		@include light-blue-gradient;
		
		&:hover {
			@include light-blue-gradient-hover;
		}
	}
}



/* PAGE LAYOUTS */
/* ------------------------------------------------------- */
#page-content {
	@include overflowfix;
	@include wrapper;
	padding: 60px 0;
	
	#left {
		float: left;
		@include box-sizing;
	}
		
	#right {
		float: right;
		@include box-sizing;
	}
}

#page-content.fifty-fifty-layout {
	
	#left {
		width: 50%;
		padding: 0 40px 0 0;
		border-right: 1px solid $light-grey;
	}
	
	#right {
		width: 50%;
		padding: 0 0 0 40px;
	}
}

#page-content.left-sidebar-layout {
	#left {
		width: 25%;
		
		aside.summary {
			margin: 0 0 40px 0;
		}
	}
	
	#right {
		width: 75%;
		padding: 0 0 0 40px;
	}
}

#page-content.right-sidebar-layout {
	#left {
		width: 75%;
		padding: 0 40px 0 0;
	}
	
	#right {
		width: 25%; 
	}
}

#page-content.full-width-layout {
	#center {
		width: 100%;
		padding: 0;
	}
}




/* HEADER */
/* ------------------------------------------------------- */
header {
	position: relative;
	height: 90px;
	
	/* start secondary nav */
	#secondary-nav {
		@include wrapper;
		
		#logo {
			position: relative;
			z-index:2;
			float: left;
		}
		
		span.reserve-by-phone {
			float: left;
			margin: 13px 0 0 0;
			font-size: 15px;
		}
		
		#register {
			float: right;
			font-weight: 600;
			margin: 15px 0 0 10px;
		}
		
		#customer-login {
			float: right;
			margin: 11px 0 0 0;
			@include small-button;
			@include light-blue-gradient;
			
			&:hover {
				@include light-blue-gradient-hover;
			}
		}
	} /* end secondary nav */
	
	/* start main nav */
	.nav {
		position:absolute;
		bottom: 0;
		left: 0;
		right: 0;
		display:inline-block;
		width: 100%;
		@include red-gradient;
		
		ul {
			margin: 0 auto;
			list-style-type: none;
			
			li {
				margin: 0;
			}
		}
	}
		
	.nav-list {
		@include wrapper;
		text-align: right;
	}
	
	.nav-item {
		display: inline-block;
		zoom:1;
		position:relative;
		font-size: 15px;
		line-height: 1;

		a {
			display: inline-block;
			padding: 11px 23px 13px;
			color: #fff;
			@include text-shadow(1px, 1px, rgba(0,0,0,0.15));

			&:hover {
				@include red-gradient-hover;
			}
			
			&#cart {
				background: #850005;
				
				&:hover {
					@include red-gradient-hover;
				}
			}
		}
		
		&.active > a, &.active > a#cart {
			@include red-gradient-hover;
		}
	}
	
	.nav-item:hover .nav-submenu {
		display: block;
	}
	
	.nav-submenu {
		display: none;
		position: absolute;
		left: 0;
		width: 225px;
		text-align: left;
		background: #b62126;
		z-index: 5;
		line-height: 1.25;
		
		a {
			display: block;
			font-size: 14px;
			padding: 8px 23px 8px;
		
			&:hover {
				background: #d0262b;
			}
		}
	}
	
	/* Mobile navigation */
	.nav-mobile {
		display:none; /* Hide from browsers that don't support media queries */
	}
	/* Mobile navigation, clickable area for revealing <ul> */
	.nav-click {
		position:absolute;
		top:0;
		right:0;
		display:none;
		height:34px;
		width:34px;
		cursor:pointer;
	}
	.nav-click i {
		display:block;
		height:34px;
		width:34px;
		background:#850005 url("../images/nav-arrow.png") no-repeat center center;
		background-size:16px;
	}
	.nav-click:hover {
		background-color:#6E1656;
	}
	.nav-rotate {
		-webkit-transform:rotate(180deg);
		-moz-transform:rotate(180deg);
		-ms-transform:rotate(180deg);
		-o-transform:rotate(180deg);
		transform:rotate(180deg);
	}
}

/* customer login dialog */
#customer-login-form {

	@include overflowfix;
	margin: 10px 0;

	input[type="text"], input[type="password"] {
		width: 48%;
		float: left;
		@include box-sizing;
		margin: 0;
		padding: 5px;
		font-size: 11px;
		-webkit-appearance: none;
		border: none;
		background: $light-grey;
		@include box-shadow-inset(0px, 1px, 3px, rgba(0, 0, 0, 0.15));
		
		&:focus {
			outline: 2px solid $light-blue;
			border: 0;
		}
	}
	
	input[type="text"] {
		margin: 0 4% 0 0;
	}
	
	#login-links {
		margin: 15px 0 0 0;
		float: left;
		width: 100%;
		text-align: center;
		
		a {
			font-size: 12px;
			text-transform: uppercase;
			color: $light-blue;
		}
	}
}



/* PAGE TITLE */
/* ------------------------------------------------------- */

#page-title {
	width: 100%;
	height: 150px;
	background: url("../images/title-bg.jpg");
	
}


/* FOOTER */
/* ------------------------------------------------------- */
footer {
	@include overflowfix;
	background: $red;
	
	#foot-one, #foot-two, #foot-three {
		float: left;
		color: #fff;
		
		a {
			display: block;
			margin: 0 0 10px 0;
			color: #fff;
			
			&:hover {
				text-decoration: underline;
			}
		}
	}
	
	#foot-one {
		font-size: 13px;
		line-height: 1.5;
		margin: 30px 0;
	}
	
	#foot-two {
		padding: 0 0 0 20px;
		margin: 30px 0 30px 20px;
		border-left: 1px solid #991b1f;
	}
	
	#foot-three {
		padding: 0 0 0 20px;
		margin: 30px 20px;
		border-left: 1px solid #991b1f;
	}
	
	#travel-agents {
		@include box-sizing;
		background: #991b1f;
		padding: 30px 30px 0;
		float: right;
		width: auto;
		height: 167px;
		text-align: left;
		
		h3 {
			text-transform: uppercase;
			font-weight: 700;
			color: #fff;
		}
		
		#travel-agents-login {
			
			input[type="text"], input[type="password"] {
				width: 110px;
				margin: 0 5px 0 0;
				padding: 5px;
				-webkit-appearance: none;
				border: 1px solid #8d0005;
				@include box-shadow-inset(1px, 1px, 3px, rgba(0,0,0,0.2));
			}
			
			input.small-button.red-button {
				border-color: #8d0005;
			}
		}
		
		#travel-agents-links {
			margin: 5px 0 0 0;
		}
		
		#travel-agents-links a {
			display: inline-block;
			color: #c7a3a5;
			font-size: 12px;
			text-transform: uppercase;
			
			&:first-child {
			margin: 0 15px 0 0;
			padding: 0 15px 0 0;
			border-right: 1px solid #8d0005;
			}
		}
	}
}


/* SIDE BAR */
/* ------------------------------------------------------- */

.bar-content {
	@include overflowfix;
	@include box-sizing;
	padding: 20px;
	background: $light-grey;
	border-top: 0;
	-webkit-border-bottom-left-radius: 4px;
	-webkit-border-bottom-right-radius: 4px;
	-moz-border-radius-bottomleft: 4px;
	-moz-border-radius-bottomright: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	
	p.pricing-details {
		@include overflowfix;
		font-weight: 700;
	
		span.left {
			float: left;
		}
		
		span.right {
			float: right;
			color: $red;
		}
		
		span.coupon-code {
			display: block;
			float: left;
			font-weight: 400;
		}
		
		&.grand-total {
	
			span.left {
				text-transform: uppercase;
				font-weight: 700;
				line-height: 1.6;
			}
			
			span.right {
				font-size: 20px;
				font-weight: 700;
				line-height: 1;
			}
		}
	}
	
	img.barcode {
		display: block;
		margin: 0 auto 20px;
	}
}


/* STANDARD FORM STYLES */
/* ------------------------------------------------------- */
.standard-form {
	width: 100%;
	@include overflowfix;
	
	fieldset {
		margin: 0 0 20px 0;
		width: 100%;
		float: left;
	}
	
	label, span.label {
		width: 50%;
		float: left;
		@include box-sizing;
		padding: 0 20px 0 0;
		margin: 0 0 20px 0;
		
		&:nth-child(2n) {
			padding: 0 0 0 20px;
		}
		
		span {
			display: block;
			text-transform: uppercase;
			color: $red;
			font-weight: 700;
			font-size: 12px;
			margin: 0 0 3px -1px;
		}
		
		input[type="text"], input[type="password"], select, textarea {
			-webkit-appearance: none;
			width: 100%;
			@include box-sizing;
			margin: 0;
			padding: 5px;
			border: none;
			background: $light-grey;
			@include box-shadow-inset(0px, 1px, 3px, rgba(0,0,0,0.15));
			@include border-radius(0);
			
			&:focus {
				outline: 2px solid $light-blue;
				border: 0;
			}
			
		}
		
		select {
			background: url("../images/tiny-blue-arrow.png") center right 8px no-repeat $light-grey;
			background-size: 8px 7px;
			cursor: pointer;
			height: 23px;
		}
		
		&.selection-type {
			width: 45%;
			padding: 0 0 0 20px;
		}
		
		&.qty {
			width: 15%;
			padding: 0 0 0 20px;
		}
	}
	
	p {
		float: left;
	}
}

.express-club-quick-login {

	label {
		width: 40%;
	}
	
	input[type="button"] {
		margin: 16px 0 0 5%;
		width: 15%;
	}
	
	p {
		float: none;
	}
}



/* BREADCRUMBS */
/* ------------------------------------------------------- */
#breadcrumbs {

	@include overflowfix;
	margin: 0 0 55px 0;
	
	a {
		float: left;
		text-transform: uppercase;
		font-weight: 600;
		
		&#current-breadcrumb {
			color: $dark-blue;
			cursor: default;
		}
	}
	
	img {
		float: left;
		margin: 0 15px;
	}
}



/* SEARCH RESULTS */
/* ------------------------------------------------------- */
.search-results {
	.result {
		background: $light-grey;
		@include box-sizing;
		padding: 20px;
		float: left;
		text-align: center;
		border: 4px solid $light-grey-darker;
		@include border-radius(4px);
		position: relative;
		
		&:last-child {
			border-color: $light-blue;
			
			&:after {
				content:"";
				display: block;
				position: absolute;
				width: 60px;
				height: 60px;
				background: url("../images/best-rate.png") center no-repeat;
				background-size: 100%;
				top: -22px;
				left: -18px;
			}
		}
		
		h2 {
			font-weight: 700;
		}
		
		h3 {
			font-size: 18px;
		}
	}
}

.search-results.two-rates {
	.result {
		width: 48%;
		
		&:first-child {
			margin: 0 4% 0 0;
		}
	}
}

.search-results.three-rates {
	.result {
		width: 31%;
		margin: 0 3.5% 0 0;
		
		&:last-child {
			margin: 0;
		}
	}
}

/* CONFIRM DETAILS/PAYMENT */
/* ------------------------------------------------------- */
.confirm-details {

	@include overflowfix;
	width: 50%;
	@include box-sizing;
	
	&.left-col {
		float: left;
		padding: 0 20px 0 0;
	}
	
	&.right-col {
		float: right;
		padding: 0 0 0 20px;
	}
	
	h6 {
		font-size: 12px;
		margin: 0 0 3px 0;
	}
}

	

/* SHOPPING CART */
/* ------------------------------------------------------- */

.cart-item {
	margin: 0 0 30px 0;
}

.bar-content {
	@include overflowfix;
}

.cart-details {
	width: 75%;
	@include box-sizing;
	padding: 0 40px 0 0;
	float: left;
	
	p:last-child {
		margin: 0;
	}
}

.cart-prices {
	width: 25%;
	@include box-sizing;
	padding: 0;
	float: right;
	@include overflowfix;
	
	ul {
		display: block;
		width: 100%;
		margin: 0 0 5px 0;
		padding: 0;
		@include overflowfix;
		
		li.label {
			display: block;
			float: left;
			width: 70%;
			padding: 0 20px 0 0;
			margin: 0;
			@include box-sizing;
			font-size: 15px;
			color: $dark-blue;
			font-weight: 600;
		}
		
		li.price {
			display: block;
			float: right;
			width: 30%;
			padding: 0;
			margin: 0;
			@include box-sizing;
			font-size: 15px;
			color: $red;
			font-weight: 600;
			text-align: right;
		}
		
		&.total {
			margin: 0;
			
			li.label, li.price {
				font-size: 19px;
				font-weight: 700;
			}
		}
	}
}

.edit-links {
	float: right;
	font-size: 12px;
	position: relative;
	top: 1px;	
	opacity: 0.8;

	a {
		color: #fff;
		
		&:hover {
			color: $dark-blue;
		}
	}
}

#grand-total {
	width: 26%;
	position:relative;
	right: 0;
	left: 74%;
	@include box-sizing;
	padding: 0 20px 0 0;
	
	.label {
		display: block;
		float: left;
		width: 70%;
		padding: 0 20px 0 0;
		margin: 0;
		@include box-sizing;
		font-size: 21px;
		color: $dark-blue;
		font-weight: 700;
	}
		
	.price {
		display: block;
		float: right;
		width: 30%;
		padding: 0;
		margin: 0;
		@include box-sizing;
		font-size: 21px;
		color: $red;
		font-weight: 700;
		text-align: right;
	}
}

.cart-page.bottom-button-group {
	@include overflowfix;
	margin: 30px auto 10px;
	float: left;
}

#card-expiration-month {
	float: left;
	width: 70%;
	margin: 0 5% 0 0;
}

#card-expiration-year {
	float: left;
	width: 25%;
}
	

/* CAR CARE SERVICES */
/* ------------------------------------------------------- */
#car-care-services-table {

	margin: 0 0 30px 0;
	@inlcude overflowfix;

	.car-care-row {
		background: $light-grey;
		@include overflowfix;			
		
		&:nth-child(2n) {
			background: $light-grey-darker;
		}
		
		> div {
			@include box-sizing;
			float: left;
		}
		
		.col-checkbox {
			width: 10%;
			padding: 20px;
			text-align: center;
		}
		
		.col-service {
			width: 50%;
			padding: 20px 0;
			
			strong {
				color: $dark-blue;
			}
			
			p {
				margin: 0 0 10px 0;
			
				&:last-child {
				margin: 0;
				}
			}
		}
		
		.col-price {
			width: 20%;
			padding: 20px 10px 20px 20px;
			text-align: right;
			font-size: 22px;
			font-weight: 600;
			color: $red;
		}
		
		.col-points {
			width: 20%;
			padding: 20px 20px 20px 10px;
			text-align: right;
			font-size: 22px;
			font-weight: 600;
			color: $red;
		}
	}
}


/* MAKE A RESERVATION DETAILS */
/* ------------------------------------------------------- */
#make-a-reservation-details {

	@include overflowfix;
	
	.full-width input.datepicker {
		width: 48%;
		margin-right: 4%;
		float: left;
		background: #eaeaea url("../images/icon-calendar.png") center right 6px no-repeat;
		background-size: 13px 13px;
	}
	
	.full-width select {
		width: 48%;
		float: left;
	}
}


/* EDIT A RESERVATION DETAILS */
/* ------------------------------------------------------- */
#edit-reservation-search-results span.search-criteria {
	color: $grey;
	display: inline-block;
	margin: 0 5px;
}
.delete-cart-item {
	cursor: pointer;
}


/* APPLICATION FORM */
/* ------------------------------------------------------- */
.standard-form#application-form {

	input[type="text"], input[type="password"], select, textarea {
		background: #fff;
		resize: none;
	}
	
	input[type="button"] {
		float: left;
		clear: left;
	}
}


/* POST TRIP SURVEY */
/* ------------------------------------------------------- */
#post-trip-survey {

	label.half-width {
		float: left; 
		clear: left;
		padding: 0 20px 0 0;
	}
	
	.contact-checkbox {
		
		input {
			display: inline-block;
		
			&:last-child {
				margin: 0 0 0 15px;
			}
		}
	}
}

#star-rating {
	width: 50%;
	float: right;
	padding: 0 0 0 20px;
	@include box-sizing;

	table {
		width: 100%;
		border-collapse: collapse;
		table-layout: fixed;
		margin: 19px 0 0 0;
		
		tr {
			border-bottom: 1px solid $light-grey-darker;
			
			&:first-child {
				border-top: 1px solid $light-grey-darker;
			}
		}
		
		td {
			width: 46%;
			padding: 8px 2%;
			line-height: 1;
			
			&.label-cell {
				text-align: right;
				font-weight: 700;
				color: $red;
				text-transform: uppercase;
				font-size: 12px;
			}
			
			&.stars-cell {
				
				a {
					display: inline-block;
					width: 16px;
					height: 16px;
					background: url("../images/star-blank.png") center no-repeat;
					background-size: 16px 16px;
					
					&.active, &:hover {
						background: url("../images/star-selected.png") center no-repeat;
						background-size: 16px 16px;
					}
				}
			}
		}
	}
}


/* EXPRESS CLUB ACCOUNT PAGES */
/* ------------------------------------------------------- */

.total-points {

	span {
		display: inline-block;
		font-size: 20px;
		font-weight: 700;
		color: $red;
		margin: 0 2px;
		position: relative;
		top: 2px;
	}
}

.express-club-member-page {
	.bar-content p:last-child {
		margin-bottom: 0;
	}
}

.edit-account-info-links {
	color: #cccccc;
	text-transform: uppercase;
	font-size: 12px;
}


/* HOME PAGE */
/* ------------------------------------------------------- */

section.homepage {
	@include box-sizing;
	@include overflowfix;
}

.homepage-top-box {
	width: 100%;
	height: 450px;
	padding: 30px 0;
	background: url("../images/home-bg.jpg") center no-repeat;
	
	.wrapper {
		height: 100%;
		@include overflowfix;
	}
	
	#search-box {
		@include box-sizing;
		height: 100%;
		background: #fff;
		width: 35%;
		float: left;
		padding: 20px;
		border: 8px solid rgba(255,255,255,0.4);
		-webkit-background-clip: padding-box;
		-moz-background-clip: padding-box;
		background-clip: padding-box;
		@include box-shadow(1px, 1px, 3px, rgba(0,0,0,0.1));
		
		h2 {
			font-size: 28px;
			font-weight: 700;
			line-height: 1;
			margin: 0 0 15px 0;
		}
		
		p {
			font-size: 18px;
			margin: 0 0 15px 0;
		}
		
		hr {
			height: 2px;
			background: $light-grey;
			margin: 15px 0 20px;
		}
		
		.bottom-button-group {
			margin-top: 11px;
		}
	}
	
	#homepage-slideshow {
		@include box-sizing;
		width: 62%;
		height: 100%;
		float: right;
		background: #fff;
		border: 8px solid rgba(255,255,255,0.4);
		-webkit-background-clip: padding-box;
		-moz-background-clip: padding-box;
		background-clip: padding-box;
		@include box-shadow(1px, 1px, 3px, rgba(0,0,0,0.1));
		position: relative;
		
		.slides > li {
			position: relative;
		}
		
		.slide-overlay {
			@include box-sizing;
			width: 455px;
			height: 100%;
			padding: 60px 20px 40px 140px;
			position: absolute;
			right: 0;
			background: url("../images/home-slider-gradient.png") right repeat-y;
			z-index: 2;
			text-align: center;
			
			h3 {
				color: #fff;
				font-size: 24px;
			}
			
			p {
				color: #fff;
				font-size: 16px
			}
			
			.mid-button {
				border: 4px solid rgba(0,0,0,0.2);
				-webkit-background-clip: padding-box;
				-moz-background-clip: padding-box;
				background-clip: padding-box;
			}
		}
	}
}

.homepage-search-form {

	label.half-width {
		width: 48%;
		float: left;
		@include box-sizing;
		padding: 0;
		margin: 0 0 10px 0;
		
		&:nth-child(2n) {
			margin: 0 0 0 4%;
		}
	}
	
	label.full-width, span.label.full-width {
		width: 100%;
		float: left;
		@include box-sizing;
		padding: 0 0 0 0;
		margin: 0 0 10px 0;
		@include overflowfix;
		
		input {
			width: 48%;
			float: left;
			margin: 0 4% 0 0;
		}
		
		select {
			width: 48%;
			float: left;
		}
	}
	
	label input.datepicker, span.label input.date-picker {
		background: #eaeaea url("../images/icon-calendar.png") center right 6px no-repeat;
		background-size: 13px 13px;
	}
}

.homepage-callouts {
	@include wrapper;
	padding: 60px 0;
	border-bottom: 1px solid $light-grey;
	
	.callout {
		float: left;
		@include box-sizing;
		width: 22.75%;
		margin: 0 3% 0 0;
		text-align: center;

		&#callout-four {
			margin: 0;
		}
		
		h3 {
			margin: 15px 0;
		}
	}
}

.homepage-testimonials-faqs {
	@include wrapper;
	padding: 60px 0;
	@include overflowfix;
	
	#testimonials {
		width: 35%;
		@include box-sizing;
		padding: 0 30px 0 0;
		border-right: 1px solid $light-grey;
		float: left;
		
		> h3 {
			color: $dark-blue;
			text-align: center;
		}
		
		.testimonial-item {
			margin: 0 0 20px 30px;
			padding: 0 0 20px 0;
			border-bottom: 1px solid $light-grey;
			
			&:before {
				content: " ";
				display: block;
				width: 22px;
				height: 20px;
				background: url("../images/quote.png") center no-repeat;
				background-size: 22px 20px;
				position: absolute;
				left: -2px;
			}
			
			&:last-child {
				border-bottom: 0;
				margin-bottom: 0;
				padding-bottom: 0;
			}
		}
		
		p {
			margin: 0 0 15px 0;
		}
		
		span.name {
			display: block;
			font-size: 12px;
			color: $red;
			font-weight: 600;
			text-align: right;
		}
	}
	
	#accordion-wrapper {
		width: 65%;
		@include box-sizing;
		padding: 0 0 0 30px;
		float: right;
	
		> h3 {
			color: $dark-blue;
			text-align: center;
		}
	}
}

.homepage-partners {
	padding: 40px 0 20px;
	background: $light-grey;
	text-align: center;
	
	h3 {
		color: $dark-blue;
	}
	
	img {
		margin: 0 20px 20px;
	}
}







