/* ------------------------------------------------------- */
/* MEDIA QUERIES */
/* ------------------------------------------------------- */
/* ------------------------------------------------------- */

/* ------------------------------------------------------- */
@media only screen and (max-width: 1140px) {

/* mixins and variables */
	@mixin wrapper {
		width: 94%;
		margin-right: 3%;
		margin-left: 3%;
	}
	
/* general layout stuff */

	.wrapper {
		@include wrapper;
	}
	
	#page-content {
		@include wrapper;
	}
	
	footer .wrapper {
		width: 97%;
		margin-right: 0;
	}
	
	header .nav-item {
		font-size: 14px;
		
		> li > a {
			padding: 11px 22px 13px;
		}
	}
		
/* homepage */
	.homepage-top-box #homepage-slideshow {
		height: auto;
	}
	.homepage-callouts, .homepage-testimonials-faqs {
		@include wrapper;
	}

}


/* ------------------------------------------------------- */
@media only screen and (max-width: 1050px) {

	/* mixins and variables */
	@mixin wrapper {
		width: 94%;
		margin-right: 3%;
		margin-left: 3%;
	}

	/* header */
	header {
		height: 130px;
		
		#secondary-nav {
			text-align: center;
			@include wrapper;
			
			#logo {
				float: none;
			}
			
			span {
				float: left;
			}
		}
		
		.nav-list {
			text-align: center;
			
			> li > a {
				padding: 25px 22px 14px;
			}
		}
	}
}



/* ------------------------------------------------------- */
@media only screen and (max-width: 970px) {

	/* header */
	header {
		height: 170px;
		
		#secondary-nav {
		
			#logo img {
				position: relative;
				top: 40px;
			}
			
			.hide-mid {
				display: none;
			}
			
			.reserve-by-phone {
				float: none;
				position: absolute;
				left: 0;
				top: 0;
			}
			
			a#register {
				float: none;
				position: absolute;
				right: 0;
				top: 0;
			}
			
			a#customer-login {
				float: none;
				position: absolute;
				right: 75px;
				top: 0;
			}
		}
	}
	
	
	/* homepage */
	.homepage-top-box #search-box {
	
		height: auto;
		padding: 20px 20px 12px;
		
		fieldset {
			margin: 0;
		}
	
		h2 {
			font-size: 24px;
		}
		
		p {
			font-size: 14px;
		}
	}
	
	.homepage-top-box #homepage-slideshow .slide-overlay {
		padding-top: 40px;
	}	
	
	/* footer */
	footer {
		text-align: center;
		
		.wrapper {
			margin-right: 0;
			margin-left: 0;
			width: 100%;
		}
		
		#foot-one, #foot-two, #foot-three {
			float: none;
			text-align: left;
			display: inline-block;
		}
		
		#travel-agents {
			float: none;
			display: block;
			width: 100%;
			text-align: center;
		}
	}
}



/* ------------------------------------------------------- */
@media only screen and (max-width: 890px) {

	/* homepage */
	.homepage-top-box {
		height: auto;
		background-size: auto 100%;
		
		#search-box {
			width: 100%;
			@include box-sizing;
		}
		
		#homepage-slideshow {
			height: auto;
			width: 100%;
			margin: 30px 0 0 0;
			
			.slide-overlay {
				padding-top: 60px;
			}
		}
	}
		
	.homepage-callouts .callout {
		width: 48%;
		margin: 0;
		text-align: left;
		
		&#callout-one, &#callout-three {
			margin: 0 4% 0 0;
		}
		
		&#callout-one, &#callout-two {
			margin-bottom: 30px;
		}
		
		img {
			float: left;
			width: 30%;
			margin: 0 5% 0 0;
		}
		
		.text-content {
			float: right;
			width: 65%;	
		}
	}
	
	.homepage-testimonials-faqs #testimonials, .homepage-testimonials-faqs #accordion-wrapper {
		width: 50%;
		border: none;
	}
}


